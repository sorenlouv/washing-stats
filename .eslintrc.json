{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "browser": false,
    "node": true
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": true
      }
    ],
    "no-console": "off"
  },
  "extends": ["eslint:recommended", "prettier", "prettier/react"]
}
